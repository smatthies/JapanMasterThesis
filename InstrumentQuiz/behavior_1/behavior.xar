class MyClass(GeneratedClass):
    solution = None
    recWord = None
    songs = None
    songChoice = None

    def __init__(self):
        GeneratedClass.__init__(self)
        global songChoice
        songChoice = int(0)
    def onLoad(self):
        #put initialization code here
        self.tablet  = self.session().service("ALTabletService")
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_startGame(self):
        #self.win() #activate the output of the box
        global songs
        self.tablet.hideImage()
        songs =  [["saxophone.ogg", "saxophone", "sax.png"],["horn.ogg","horn","horn.png"], ["trompete.ogg","trumpet", "trumpet.png"]]
        global songChoice
        if songChoice <= (len(songs) - 1):
            song = songs[songChoice]
            self.playSong( "/../sounds/" + song[0])
            time.sleep(7)

            url = self.behaviorAbsolutePath().split("apps/")
            if len(url) > 1:
                url = str( url[1].split("/")[0] ) + "/"
                url = "http://198.18.0.1/apps/" + url
            self.Solution(song[1])
            time.sleep(15)
            self.tablet.showImage(url + song[2])
        else:
            self.end()
        pass

    def compare(self):
        global songs
        global songChoice
        song = songs[songChoice]
        solution = song[1]
        if (solution != None and recWord != None):
            if (solution == recWord):
                url = self.behaviorAbsolutePath().split("apps/")
                url = str( url[1].split("/")[0] ) + "/"
                url = "http://198.18.0.1/apps/" + url
                self.tablet.showImage(url  + song[2])
                sentences = ["Not bad! That was right.", "Exactly.","Thats true!","Right!"]
                length = len(sentences) - 1
                randint = random.randint(0,length)
                self.say(sentences[randint])
                self.say("The instrument is called " + song[1])
                time.sleep(3)
                self.say("Here is the sound again.")
                time.sleep(6)
                songChoice = songChoice + 1
                self.win("/../sounds/"+song[0])
                pass
            else:
                self.fail()
                self.Solution(song[1])
        pass

    def onInput_recWord(self, word):
        global recWord
        recWord = word
        self.compare()
        pass

    def onInput_repeat(self):
        global songs
        global songChoice
        song = songs[songChoice]
        self.playSong("/../sounds/" + song[0])
        pass]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="startGame" type="1" type_size="1" nature="1" inner="0" tooltip="Box behavior starts when a signal is received on this input." id="2" /><Input name="recWord" type="3" type_size="1" nature="1" inner="0" tooltip="" id="3" /><Input name="repeat" type="1" type_size="1" nature="1" inner="0" tooltip="" id="4" /><Output name="win" type="3" type_size="1" nature="2" inner="0" tooltip="Signal sent when box behavior is finished." id="5" /><Output name="say" type="3" type_size="1" nature="2" inner="0" tooltip="" id="6" /><Output name="fail" type="1" type_size="1" nature="2" inner="0" tooltip="" id="7" /><Output name="playSong" type="3" type_size="1" nature="2" inner="0" tooltip="" id="8" /><Output name="end" type="1" type_size="1" nature="2" inner="0" tooltip="" id="9" /><Output name="Solution" type="3" type_size="1" nature="2" inner="0" tooltip="" id="10" /><Output name="hint" type="3" type_size="1" nature="2" inner="0" tooltip="" id="11" /></Box><Box name="Play Sound" id="17" localization="8" tooltip="Play a sound file. Select the file in parameters. The format of the file can be wav or ogg (on virtual robot) and also mp3 (on a real robot).&#x0A;&#x0A;Note: There can be some delay to play ogg or mp3 (on a robot) files. We advise you to use wav if&#x0A;you want a fast play of the file." x="498" y="71"><bitmap>media/images/box/interaction/play_music.png</bitmap><script language="4"><content><![CDATA[class MyClass(GeneratedClass):
